	screen -S tfc -p 0 -X stuff "`printf "/tp initech 38908 153 297\r"`"
fi
if [ "$1" == 'b' ]; then
	screen -S tfc -p 0 -X stuff "`printf "/tp initech 38923 147 4285\r"`"
fi
